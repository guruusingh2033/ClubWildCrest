@*@model WildCrest.Models.WildCrestModels.Profile*@
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

</head>
<body class="fix-header card-no-border">
    
        <div class="row">
            <div class="form-body">
                <h3 class="card-title">Profile</h3>
                <div class="admin-form-back">
                    @*<div class="row p-t-20">*@
                    <div class="col-md-5 form-profile-in">
                        <div class="form-group">
                            <label class="control-label">First Name</label>
                            <input type="text" id="firstName" disabled="disabled" value="@Model.F_Name" class="form-control">

                        </div>
                    </div>

                    <div class="col-md-5 form-profile-in">
                        <div class="form-group">
                            <label class="control-label">Last Name</label>
                            <input type="text" id="lastName" disabled="disabled" value="@Model.L_Name" class="form-control">

                        </div>
                    </div>
                    @*</div>*@

                    <div class="col-md-5 form-profile-in">
                        <div class="form-group">
                            <label class="control-label">DOB :</label>
                            <input type="text" id="DOB" disabled="disabled" value="@Model.DOB" class="form-control">

                        </div>
                    </div>
                    <!--/span-->
                    <div class="col-md-5 form-profile-in">
                        <div class="form-group">
                            <label class="control-label">Email :</label>
                            <input type="text" id="Email" disabled="disabled" value="@Model.Email" class="form-control">

                        </div>
                    </div>

                    <div class="col-md-5 form-profile-in">
                        <div class="form-group">
                            <label class="control-label">Phone :</label>
                            <input type="text" id="Phone" disabled="disabled" value="@Model.PhoneNo" class="form-control">

                        </div>
                    </div>
                    <!--/span-->
                    <div class="col-md-5 form-profile-in">
                        <div class="form-group">
                            <label class="control-label">Address :</label>
                            <input type="text" id="Address" disabled="disabled" value="@Model.Address" class="form-control">

                        </div>
                    </div>
                    <!--/span-->
                    <div class="col-md-5 form-profile-in">
                        <div class="form-group">
                            <label class="control-label">City :</label>
                            <input type="text" id="City" disabled="disabled" value="@Model.City" class="form-control">

                        </div>
                    </div>
                    <div class="col-md-5 form-profile-in">
                        <div class="form-group">
                            <label class="control-label">State :</label>
                            <input type="text" id="State" disabled="disabled" value="@Model.State" class="form-control">

                        </div>
                    </div>
                    <br/>
                   @Html.ActionLink("Edit Profile", "EditProfilebyID", "UserProfile", new { id = Model.ID }, new { @class = "btn btn-primary btn-margin" })
                </div>
            </div>
        </div>
    </body>

















   
